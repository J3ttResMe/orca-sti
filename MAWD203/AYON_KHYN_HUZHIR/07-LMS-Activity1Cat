//Khyn's Cats

//1st Cat Unknown '-'
//declaration
public class Cat {
    private String catType;
    private int cutenessLvl;

//setter
//without parameters
    public Cat() {
        this.catType = "Unknown";
        this.cutenessLvl = 3;
    }

//with parameters
    public Cat(String type, int lvl) {
        this.catType = type;
        this.cutenessLvl = lvl;
    }

//getter
    public int getCuteness() {
        return this.cutenessLvl;
    }


    public String getCatType() {
        return this.catType;
    }

//2nd Cat My Hikari

//setter
    public void setCatType(String type) {
        this.catType = type;
    }


    public void setCutenessLvl(int lvl) {
        this.cutenessLvl = lvl;
    }

//getter and printer
    public static void main(String[] args) {
        
        Cat cat1 = new Cat();
        System.out.println("Cat1 Type: " + cat1.getCatType()); 
        System.out.println("Cat1 Cuteness: " + cat1.getCuteness()); 
        System.out.println();
        
        System.out.println("∞∞∞∞∞∞∞∞∞∞∞∞∞My Cats∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞");
        
        System.out.println();
        Cat cat2 = new Cat("ヒカリ", 20);
        System.out.println("Cat2 Type: " + cat2.getCatType()); 
        System.out.println("Cat2 Cuteness: " + cat2.getCuteness());
    }
}
